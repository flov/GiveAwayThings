- form_tag "/#{search}/", :method => 'get' do
  %table.welcome_form
    %tr
      %td
        .in_field_label
          %label{:for=>"#{search}"}==search #{search}
          = text_field_tag :search, nil, :size => 36, :id=>"#{search}"
      %td
        = image_submit_tag "searchbutton.png", :class => 'giveaway_submit'
